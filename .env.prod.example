# Application settings
APP_NAME=fastapi-app
APP_VERSION=1.0.0
APP_ENV=production
DEBUG=False

# Server settings
HOST=0.0.0.0
PORT=80

# Database settings
CLICKHOUSE_HOST="v_clickhouse"
CLICKHOUSE_PORT=8123
CLICKHOUSE_USERNAME="root"
CLICKHOUSE_PASSWORD="111-111-111"
CLICKHOUSE_DATABASE="default"
CLICKHOUSE_SECURE=False

# JWT settings
JWT_SECRET_KEY=replace_with_secure_production_key
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=15

# CORS settings - restrict to your frontend domains
CORS_ORIGINS=["https://yourdomain.com", "https://api.yourdomain.com"]

# Logging
LOG_LEVEL=WARNING

# Gunicorn settings
WORKERS_PER_CORE=1
MAX_WORKERS=2
WORKER_CONNECTIONS=1000
TIMEOUT=60
KEEP_ALIVE=5
GRACEFUL_TIMEOUT=120

# Tor proxy settings (free tier)
TOR_PROXY_HOST=tor-proxy
TOR_PROXY_PORT=9050

# Paid proxy provider selection
# Options: brightdata, oxylabs, smartproxy, iproyal, floppydata
PROXY_PROVIDER=floppydata

# Bright Data ($15/GB) - https://brightdata.com
BRIGHTDATA_HOST=brd.superproxy.io
BRIGHTDATA_PORT=22225
BRIGHTDATA_USERNAME=
BRIGHTDATA_PASSWORD=

# Oxylabs ($12/GB) - https://oxylabs.io
OXYLABS_HOST=pr.oxylabs.io
OXYLABS_PORT=7777
OXYLABS_USERNAME=
OXYLABS_PASSWORD=

# Smartproxy ($8/GB) - https://smartproxy.com
SMARTPROXY_HOST=gate.smartproxy.com
SMARTPROXY_PORT=7000
SMARTPROXY_USERNAME=
SMARTPROXY_PASSWORD=

# IPRoyal ($5/GB) - https://iproyal.com
IPROYAL_HOST=geo.iproyal.com
IPROYAL_PORT=12321
IPROYAL_USERNAME=
IPROYAL_PASSWORD=

# FloppyData ($1/GB) - https://floppydata.ai
# Ports: 10080 (HTTP), 10443 (HTTPS), 10800 (SOCKS5)
FLOPPYDATA_HOST=geo.g-w.info
FLOPPYDATA_PORT=10080
FLOPPYDATA_USERNAME=
FLOPPYDATA_PASSWORD=
FLOPPYDATA_COUNTRY=US
FLOPPYDATA_CITY=              # Optional: e.g., New_York (use underscore for spaces)
FLOPPYDATA_ROTATION=0         # 0=sticky session, -1=per-request, 1-60=auto-rotate minutes
